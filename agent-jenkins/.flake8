[flake8]
select = B,B9,C,D,DAR,E,F,N,RST,W
ignore = W503,B904,E203,E501,RST201,RST203,RST301,W503,D212,B950,C901,N803,D417,RST306,B905,N806,N817,B907,RST208,B012,N802,N818,E266,RST210,RST213,N814,N813,D100,D104,D100
max-line-length = 100
max-complexity = 10
docstring-convention = google
rst-roles = class,const,func,meth,mod,ref
rst-directives = deprecated
